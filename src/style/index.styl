      .aside ul {
        padding: 20px 16px;
        margin: 0;
        list-style: none;
      }
      .aside ul li {
        display: flex;
        align-items: center;
        justify-content: center;
        border-radius: 6px;
        padding: 0;
        margin-bottom: 8px;
        cursor: pointer;
        border-radius: 6px;
        border-color: transparent;
        background-size: 110%, 100%;
        background-position: 50%;
        position: relative;
        background-color: rgba(194, 202, 242, 0.1);
        transition: all 0.25s;
      }
      .aside ul li.active {
        background-image: var(--vscode-icube-colorBgDefaultGradient);
      }
      .aside ul li.active a {
        background-color: #181a1f;
        background-color: rgb(36, 38, 43);
      }

