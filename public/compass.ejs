      .home-distance-wrapper {
        color: #fff;
        z-index: 1;
        justify-content: flex-start;
        align-items: center;
        font-size: 14px;
        line-height: 14px;
        display: flex;
      }
      .home-distance-wrapper .home-distance-text {
        margin-left: 4px;
        padding: 1px 0;
        font-size: 12px;
        font-weight: 700;
        line-height: 20px;
      }
      .gimbal-icon {
        display: block;
      }
      .gimbal-icon:before {
        content: '';
        border-bottom: calc(0.5em + 1px) solid transparent;
        border-left: 0.5em solid transparent;
        border-right: 0.5em solid transparent;
        width: 0;
        height: 0;
        display: block;
        position: absolute;
        bottom: 100%;
      }
      .gimbal-icon .gimbal-icon-inner {
        background-color: #1fa3f6;
        width: 1em;
        height: 1em;
        margin: 0.5px 1px 1px;
        display: block;
        position: relative;
      }
      .gimbal-icon .gimbal-icon-inner:before {
        content: '';
        border-bottom: 0.5em solid #1fa3f6;
        border-left: 0.5em solid transparent;
        border-right: 0.5em solid transparent;
        width: 0;
        height: 0;
        display: block;
        position: absolute;
        bottom: 100%;
      }
      .pitch-bar-wrapper {
        padding-left: 7px;
        padding-right: 5px;
      }
      .pitch-bar-wrapper .pitch-bar-text {
        text-align: center;
        color: #00ee8b;
        width: 80px;
        margin-left: -34px;
        font-size: 16px;
        font-weight: 700;
        line-height: 24px;
        position: absolute;
        top: -25px;
      }
      .pitch-bar {
        color: rgba(255, 255, 255, 0.2);
        width: 7px;
        height: 73px;
        box-shadow: 0 0 0 0.4px rgba(0, 0, 0, 0.6);
      }
      .marker {
        background-color: #fff;
        width: 7px;
        height: 2px;
        position: absolute;
        transform: translateY(-50%);
        box-shadow: 0 0 0 0.4px rgba(0, 0, 0, 0.6);
      }
      .marker.zero-marker {
        width: auto;
        top: 50%;
        left: 7px;
        right: 0;
      }
      .gimbal-icon {
        position: absolute;
        left: -6px;
        transform: rotate(90deg) translate(-50%);
      }
      .hsi-middle-wrapper {
        justify-content: center;
        align-items: center;
        width: 380px;
        height: 100%;
        padding-left: 42px;
        display: flex;
        position: relative;
      }
      .hsi-middle-wrapper .pitch-bar-wrapper {
        position: absolute;
        top: 66px;
        left: 96px;
      }
      .hsi-middle-wrapper .home-distance-wrapper {
        position: absolute;
        bottom: 0;
        left: 250px;
      }
      .hsi-middle-wrapper .hsi {
        width: 180px;
        height: 180px;
        position: relative;
        overflow: hidden;
      }
      .hsi-middle-wrapper .hsi .hsi-compass {
        z-index: 1;
        position: absolute;
        top: 0;
        left: 0;
      }
      .hsi-middle-wrapper .hsi .hsi-direction-angle {
        text-align: center;
        color: #00ee8b;
        z-index: 2;
        width: 146px;
        font-size: 16px;
        font-weight: 700;
        line-height: 24px;
        position: absolute;
      }
      .hsi-middle-wrapper .hsi .horizontal-obstacle-avoidance {
        z-index: 2;
        position: absolute;
        top: 55px;
        left: 23px;
      }
      .hsi-middle-wrapper .hsi .vertical-obstacle-avoidance {
        z-index: 2;
        position: absolute;
        top: 66px;
        left: 152px;
      }
      .hsi-middle-wrapper .hsi .vertical-velocity-vector {
        z-index: 2;
        position: absolute;
        top: 78px;
        left: 165px;
